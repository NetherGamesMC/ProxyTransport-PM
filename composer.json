  "license": "GPL-3.0-or-later",
  "type": "project",
  "require": {
    "php": "^8.1",
    "ext-pmmpthread": "^6.0.1",
    "ext-sockets": "*",
    "nethergamesmc/libproxy": "dev-quic"
  },
  "require-dev": {
    "phpstan/phpstan": "1.11.1",
    "nethergamesmc/pocketmine-mp": "dev-stable"
  },
  "repositories": [
    { "type": "vcs", "url": "git@github.com:NetherGamesMC/PocketMine-MP.git" },
    { "type": "vcs", "url": "git@github.com:NetherGamesMC/libproxy.git" }
  ],
  "autoload": {
    "psr-4": {
      "": ["src"]
    },
    "files": [
      "src/NetherGames/ProxyTransport/CoreConstants.php"
    ]
  },
  "minimum-stability": "dev"
